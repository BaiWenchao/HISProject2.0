      <Label fx:id="doctor" layoutX="749.0" layoutY="17.0" />
      <Button fx:id="turnToReDiagnosis" layoutX="1073.0" layoutY="688.0" mnemonicParsing="false" text="等待复诊" />
   </children>
</AnchorPane>
